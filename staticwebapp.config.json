{
    "routes": [
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/*.{png,jpg,gif,svg}",
        "/*.{css,scss}",
        "/*.js",
        "/*.wasm"
      ]
    },
    "globalHeaders": {
      "Cache-Control": "no-cache, no-store, must-revalidate",
      "Pragma": "no-cache",
      "Expires": "0"
    },
    "mimeTypes": {
      ".wasm": "application/wasm"
    }
  }